{
  "name": "personal-reading-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Personal Reading Tracker",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "seed": "tsx src/scripts/seed.ts seed",
    "seed:reading-list": "tsx src/scripts/seed.ts seed-reading-list",
    "seed:clear": "tsx src/scripts/seed.ts clear",
    "seed:reset": "tsx src/scripts/seed.ts reset",
    "seed:reset-reading-list": "tsx src/scripts/seed.ts reset-reading-list",
    "seed:backup": "tsx src/scripts/seed.ts backup",
    "seed:add-progress": "tsx src/scripts/seed.ts add-progress",
    "seed:parse-kindle": "tsx src/scripts/seed.ts parse-kindle"
  },
  "keywords": [
    "reading",
    "tracker",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
